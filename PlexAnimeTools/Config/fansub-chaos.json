{
  "TMDbAPIKey": "YOUR_TMDB_API_KEY_HERE",
  "JikanRateLimit": 500,
  "TMDbRateLimit": 300,
  "VideoExtensions": [
    ".mkv",
    ".mp4",
    ".avi",
    ".m4v",
    ".mov",
    ".wmv",
    ".flv",
    ".webm",
    ".ts",
    ".ogm",
    ".asf",
    ".mpg",
    ".mpeg"
  ],
  "NamingFormat": {
    "Episode": "{Title} - S{Season:D2}E{Episode:D2} - {EpisodeTitle} [{Quality}]",
    "Movie": "{Title} ({Year}) [{Quality}]",
    "Special": "{Title} - S00E{Episode:D2} - {EpisodeTitle}"
  },
  "FolderStructure": {
    "Series": "{OutputPath}\\{Title}\\Season {Season:D2}",
    "Movie": "{OutputPath}\\Movies\\{Title}",
    "RelatedMovie": "{OutputPath}\\{Title}\\Season 00"
  },
  "SearchSettings": {
    "RemoveTags": [],
    "RemoveQuality": [],
    "RemoveSeasonInfo": true,
    "PreserveGroupTags": true
  },
  "AutoDetection": {
    "AnimeKeywords": [
      "anime",
      "season",
      "episode",
      "ep",
      "ova",
      "ona",
      "special",
      "BD",
      "BluRay",
      "BDRip"
    ],
    "AnimeGroups": [
      "HorribleSubs",
      "SubsPlease",
      "Erai-raws",
      "Commie",
      "FFF",
      "GJM",
      "Judas",
      "Doki",
      "gg",
      "Coalgirls",
      "UTW"
    ],
    "MovieKeywords": [
      "movie",
      "film"
    ]
  },
  "ErrorHandling": {
    "ContinueOnError": true,
    "LogErrors": true,
    "LogPath": "Desktop",
    "DetailedErrorLog": true
  },
  "Features": {
    "DownloadArtwork": true,
    "CreateFolders": true,
    "MoveFiles": true,
    "RenameFiles": true,
    "BackupOriginalNames": true
  },
  "Advanced": {
    "UseEnglishTitles": false,
    "FallbackToRomaji": true,
    "StripParentheses": false,
    "MaxConcurrentAPI": 1,
    "RetryOnFailure": 5,
    "RetryDelayMs": 2000
  }
}
